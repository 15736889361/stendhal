<?xml version="1.0" encoding="ISO-8859-1"?><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"                   elementFormDefault="qualified">	<xsd:element name="items" type="Items"/>	<xsd:element name="item" type="Item"/>	<xsd:complexType name="Items">		<xsd:sequence>			<xsd:element name="item" type="Item" maxOccurs="unbounded"/>		</xsd:sequence>	</xsd:complexType>		<xsd:complexType name="Item">		<xsd:sequence>			<xsd:element name="type" type="Type" minOccurs="1"/>			<xsd:element name="description" type="xsd:string" minOccurs="0"/>			<xsd:element name="stackable" type="xsd:string" minOccurs="0"/>			<xsd:element name="attributes" type="Attributes" minOccurs="0"/>			<xsd:element name="weight" type="Weight" minOccurs="1"/>			<xsd:element name="equipable" type="Equipable" minOccurs="1"/>		</xsd:sequence>		<xsd:attribute name="name" type="xsd:string"/>	</xsd:complexType>	<xsd:complexType name="Type">		<xsd:attribute name="class" type="xsd:string"/>		<xsd:attribute name="subclass" type="xsd:string"/>	</xsd:complexType>	<xsd:complexType name="Attributes">		<xsd:sequence>			<xsd:element name="atk" minOccurs="0">				<xsd:complexType>					<xsd:attribute name="value" type="xsd:positiveInteger"/>				</xsd:complexType>			</xsd:element>			<xsd:element name="def" minOccurs="0">				<xsd:complexType>					<xsd:attribute name="value" type="xsd:positiveInteger"/>				</xsd:complexType>			</xsd:element>			<xsd:element name="hp" minOccurs="0">				<xsd:complexType>					<xsd:attribute name="value" type="xsd:positiveInteger"/>				</xsd:complexType>			</xsd:element>			<xsd:element name="speed" minOccurs="0">				<xsd:complexType>					<xsd:attribute name="value" type="xsd:decimal"/>				</xsd:complexType>			</xsd:element>			<xsd:element name="size" minOccurs="0">				<xsd:complexType>					<xsd:attribute name="value" type="xsd:string"/>				</xsd:complexType>			</xsd:element>			<xsd:element name="quantity" minOccurs="0">				<xsd:complexType>					<xsd:attribute name="value" type="xsd:positiveInteger"/>				</xsd:complexType>			</xsd:element>			<xsd:element name="amount" minOccurs="0">				<xsd:complexType>					<xsd:attribute name="value" type="xsd:integer"/>				</xsd:complexType>			</xsd:element>			<xsd:element name="regen" minOccurs="0">				<xsd:complexType>					<xsd:attribute name="value" type="xsd:integer"/>				</xsd:complexType>			</xsd:element>			<xsd:element name="frequency" minOccurs="0">				<xsd:complexType>					<xsd:attribute name="value" type="xsd:positiveInteger"/>				</xsd:complexType>			</xsd:element>			<xsd:element name="infostring" minOccurs="0">				<xsd:complexType>					<xsd:attribute name="value" type="xsd:string"/>				</xsd:complexType>			</xsd:element>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="Weight">		<xsd:attribute name="value" type="xsd:decimal"/>	</xsd:complexType>	<xsd:complexType name="Equipable">		<xsd:sequence>			<xsd:element name="slot" type="Slot" maxOccurs="7"/>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="Slot">		<xsd:attribute name="name" type="xsd:string"/>	</xsd:complexType></xsd:schema>